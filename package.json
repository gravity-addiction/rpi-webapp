  "scripts": {
    "ng": "ng",
    "start": "npm run webapp",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "init": "npm run build && npm run api",
    "webapp": "node dist/api/index.js --app_folder \"./dist/web\" --env ./src/.env",
    "api": "node_modules/webpack-cli/bin/cli.js ./src/api/api.ts --output ./dist/api/index.js --config ./api-webpack.js --env ./dist/api/.env",
    "build": "ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf yarn-lock.json && yarn install && npm start"
  },
